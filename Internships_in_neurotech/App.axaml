<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Internships_in_neurotech.App"
             xmlns:local="using:Internships_in_neurotech"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>

	<Application.Resources>

		<SolidColorBrush x:Key="MediumGreyBrush">#e5e5e5</SolidColorBrush>

		<SolidColorBrush Color="#35244E" x:Key="MainBackgroundColor"/>
		<SolidColorBrush Color="#58227C" x:Key="SecondaryBackgroundColor"/>

		<SolidColorBrush Color="#381450" x:Key="ShadowColor"/>
		
		<SolidColorBrush Color="#C4C4C4" x:Key="TextColor"/>
		
		<SolidColorBrush Color="#721BAC" x:Key="ButtonColor"/>
		<SolidColorBrush Color="#601791" x:Key="ButtonPointOverColor"/>
		<SolidColorBrush Color="#4E087C" x:Key="ButtonPressedColor"/>
		
		<SolidColorBrush Color="#58227C" x:Key="FlyoutBackgroundColor" Opacity="0.9"/>
		
		
		
		<!--2E2E2E 404040 C4C4C4-->
		<!--680FA4 303030 252525-->

	</Application.Resources>
	
</Application>